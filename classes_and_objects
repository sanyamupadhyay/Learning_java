package unit_01;
public class P5_Task_01_ClassesAndObjectsInJava{
       public static void main (String[] args){
              ABC obj=new ABC2();
              obj.dsplay2();
              ABC2.display();
       }
}
class ABC2{
      int a=10;
      static int b=10;
      static void display(){
             int b=10;
             b++;
             System.out.println(b);
      }
     void display2() {
     System.out.println(a);
    }
}
