package test;

public class abc {

	public static void main(String[] args) {

       System.out.println("hello world");
       //Syntax error on token "double", invalid VariableDeclaratorId
        int double = 2.2f;
       // Syntax error on token "int", delete this token
      Sampleclass int =new Sampleclass();
}
}
//Syntax error on token "class", record expected
class sampleclass
{
	
	public static void shivi(String[] args) {
	//Syntax error on token "int", invalid AnnotationName
	void int ()
	{
		System.out.println("hye....");
		
	}
	}
}
//Syntax error on token "int", Identifier expected
class int{
	//Syntax error on token "double", Identifier expected
	public static void double(String[] args) {
	}
}
}


class shivi
{
	
	public static void aaa(String[] args) {
		/*Multiple markers at this line
		- Syntax error on token "double", delete 
		 this token
		- shivi cannot be resolved to a variable*/
		shivi double=new shivi();
	}
	System.out.println("hiii... hiii.... hiii.... hiiii...");
	//Syntax error on token "void", invalid VariableDeclaratorId
	int void =98;
}
