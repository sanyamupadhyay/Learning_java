package unit_01;
public class P5_Task_02_MethodsInJava{
       public static void main(String[] args){
            ABC3 obj=new ABC3();
            obj.display();
            System.out.println(obj.a);
       }
}
class ABC3 {
   
         static int a=10;
         static void display(){
         int b=10;
         int a=10;
         System.out.println(b);
         System.out.println(a);
        }
      int display(){
          System.out.println(a);
          return a;
      }
}
